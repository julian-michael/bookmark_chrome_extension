{
  "manifest_version": 3,
  "name": "Bookmark Manager",
  "version": "1.0",
  "description": "A simple bookmark extension.",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png"
    }
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icon.png"
  }
}
